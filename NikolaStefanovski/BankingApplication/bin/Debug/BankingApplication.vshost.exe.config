<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IMyService" />
                <binding name="BasicHttpsBinding_IMyService">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:53423/MyService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMyService" contract="MyService.IMyService"
                name="BasicHttpBinding_IMyService" />
            <endpoint address="https://localhost:44300/MyService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpsBinding_IMyService" contract="MyService.IMyService"
                name="BasicHttpsBinding_IMyService" />
        </client>
    </system.serviceModel>
</configuration>